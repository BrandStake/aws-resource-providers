{
    "typeName": "Community::EC2::CoreNetworkRoute",
    "description": "An AWS::EC2::Route connected to a AWS::NetworkManager::CoreNetwork",
    "sourceUrl": "https://github.com/org-formation/aws-resource-providers.git",
    "replacementStrategy": "delete_then_create",
    "definitions": {},
    "properties": {
        "DestinationIpv6CidrBlock": {
            "type": "string"
        },
        "DestinationCidrBlock": {
            "type": "string"
        },
        "RouteTableId": {
            "type": "string"
        },
        "VpcAttachmentId": {
            "type": "string"
        },
        "MaxWaitSeconds": {
            "type": "number"
        },
        "CoreNetworkArn": {
            "type": "string"
        },
        "Id": {
            "type": "string"
        }
    },
    "additionalProperties": false,
    "required": [
        "RouteTableId",
        "CoreNetworkArn"
    ],
    "createOnlyProperties": [
        "/properties/RouteTableId",
        "/properties/CoreNetworkArn",
        "/properties/DestinationCidrBlock",
        "/properties/DestinationIpv6CidrBlock"
    ],
    "readOnlyProperties": [
        "/properties/Id"
    ],
    "primaryIdentifier": [
        "/properties/Id"
    ],
    "handlers": {
        "create": {
            "permissions": [
                "ec2:CreateRoute",
                "networkmanager:GetVpcAttachment"
            ]
        },
        "read": {
            "permissions": [ ]
        },
        "update": {
            "permissions": [ ]
        },
        "delete": {
            "permissions": [
                "ec2:DeleteRoute"
            ]
        }
    }
}